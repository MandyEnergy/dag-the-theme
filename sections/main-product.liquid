{%- style -%}
  .go-to-cart {
    display: none;
  }
  .js-ajax-cart-not-empty .go-to-cart {
    display: flex;
  }
  .loader {
    display: none;
  }
  .js-ajax-cart-request-in-progress .loader {
    display: block; 
  }
{%- endstyle -%}

<article>
  <section class="container px-5 py-8 lg:py-16 grid lg:grid-cols-2 gap-4 lg:gap-10 xl:gap-16 auto-rows-auto">
  <header class="lg:col-start-2 lg:row-start-1">
    <div class="mb-4">
      {% render 'breadcrumbs' %}
    </div>
    <h1 class="mb-4">
      <span class="text-gray-500">Köp</span><br /> 
      {{ product.title | escape }}
    </h1>
    <a href="#shopify-product-reviews">Läs vad våra kunder tycker &raquo;</a>
  </header>
  
  <div class="lg:col-start-2 lg:row-start-2"> 
    <form action="/cart/add" method="post" enctype="multipart/form-data" class="mb-8">
