          <span class="text-base">Snabba leveranser</span>
        </li>
      </ul> 
      <h2><label for="Qantity" class="block mb-4 font-fredoka-one text-lg normal-case">Hur många vill du ha?</label></h2>
      <select id="Quantity" name="quantity" class="mb-4 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5">
        <option>1</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        <option>5</option>
        <option>6</option>
        <option>7</option>
        <option>8</option>
        <option>9</option>
        <option>10</option>
        <option>11</option>
        <option>12</option>
      </select>
      {% if product.has_only_default_variant %}
        <input name="id" value="{{ product.selected_or_first_available_variant.id }}" type="hidden">
      {% else %}
        <!--  loop through options ex: size / color / material /  -->
        {% for product_option in product.options_with_values %}
            {{product_option.name}}
            <select name="id" multiple>
                    <!-- loop through each option value ex: size :sm,md,xl / color:blue,green,pink -->
                      <!-- we could use (for value in product_option.value) but that will not give us the id -->
                    {%for variant in product.variants%}
                        <option value="{{variant.id}}">
                          {{variant.title}}
                        </option>
                    {%endfor%}
            </select>
        {% endfor %}
      {% endif %} 
      <div data-ajax-cart-messages="form"></div>
      <div class="loader mb-4" role="status">
          <svg aria-hidden="true" class="mr-2 w-8 h-8 text-primary-100 fill-primary-900 animate-spin" viewBox="0 0 100 101" fill="none" xmlns="http://www.w3.org/2000/svg">
