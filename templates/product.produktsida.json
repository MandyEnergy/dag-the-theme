    "94d21598-b26a-4c02-94e6-c14731165f10": {
      "type": "mandy-faq",
      "blocks": {
        "template--16733939335384__94d21598-b26a-4c02-94e6-c14731165f10-1676131967daeb2adc-0": {
          "type": "q-and-a",
          "settings": {
            "question": "Hur fungerar Mandy?",
            "answer": "Lägg en Mandy i vatten och vänta till hela brustabletten har lösts upp i vattnet. När brustabletten har lösts upp så är drycken redo att drickas."
          }
        },
        "template--16733939335384__94d21598-b26a-4c02-94e6-c14731165f10-1676131967daeb2adc-1": {
          "type": "q-and-a",
          "settings": {
            "question": "Hur mycket vatten ska jag ha till en Mandy?",
            "answer": "Brustabletten är framtagen för att upplösas i 35 cl vatten. Men det går bra att justera mängden utifrån hur mycket du vill att den ska smaka."
          }
        },
        "6be89da8-e3fa-432e-909b-120f38d85d6a": {
          "type": "q-and-a",
          "settings": {
            "question": "Hur många kalorier innehåller en Mandy?",
            "answer": "0, noll eller zero kalorier."
          }
        },
        "7710748d-d82a-4bce-b9b8-d09cd783958a": {
          "type": "q-and-a",
          "settings": {
            "question": "Är Mandy veganskt?",
            "answer": "Mandy innehåller inga animaliska ämnen och är således veganskt. "
          }
        },
        "ff8440cb-4773-4c7b-9705-e2a7dc70d84e": {
          "type": "q-and-a",
          "settings": {
            "question": "Innehåller Mandy sötningsmedel?",
            "answer": "Mandy innehåller sötningsmedlet sukralos."
          }
        },
        "c55c20cb-1e67-4dab-b925-78be3d4238ab": {
          "type": "q-and-a",
          "settings": {
            "question": "Kan jag dela en brustablett i flera mindre delar? ",
            "answer": "Givetvis! Du bestämmer. Tänk på att det påverkan smaken om du inte tar en lika stor del mindre vatten."
          }
        },
        "13348959-7856-44ec-9377-4abd16383f57": {
          "type": "q-and-a",
          "settings": {
            "question": "Kan jag ta en Mandy vilken tid på dagen som helst?",
            "answer": "Du kan konsumera en Mandy när som helst under dagen när det passar dig bäst. "
          }
        },
        "c24b4b0f-e4a5-4fff-8219-0172a4bfa9e9": {
          "type": "q-and-a",
          "settings": {
            "question": "Går det att diska flaskan i diskmaskin?",
            "answer": "Det är inget vi rekommenderar men vi har fått rapporter från kunder som gör det."
          }
        },
        "1615cd59-d52f-4235-a828-08a048c60e1c": {
          "type": "q-and-a",
          "settings": {
            "question": "Kan jag blanda med kolsyrat vatten?",
            "answer": "Det går superbra att blanda med kolsyrat vatten! Go nuts."
          }
        }
      },
