    "19fd96f1-e335-474f-b86e-e0ea7e0bf666": {
      "type": "mandysection",
      "blocks": {
        "template--16733939335384__19fd96f1-e335-474f-b86e-e0ea7e0bf666-1676130334c75967bd-3": {
          "type": "section-tile-third",
          "settings": {
            "title": "Koffein motverkar trötthet.",
            "badge": "Artikel",
            "text": "<p>Koffein finns naturligt i bl.a. kaffe, te, maté, guarana och mörk choklad och motverkar trötthet. <\/p>",
